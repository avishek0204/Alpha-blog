<h1> Edit article </h1>


<%= form_with model: @article, url: article_path, method: :patch, local: true do |f| %>
    <p>
        <%= f.label :title %>
        <%= f.text_field :title%>
    </p>

    <p>
        <%= f.label :description %>
        <%= f.text_area :description %>
    </p>

    <p>
        <%= f.submit %>
    </p>
<% end %>


